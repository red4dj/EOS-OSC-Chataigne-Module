{
	"name": "EOS OSC",
	"type": "OSC",
	"path": "Software",

	"version": "1.0.3",
	"description": "Mike's special module to control EOS Light Console through OSC via commands",
	"url": "https://github.com/benkuper/EOS-OSC-Chataigne-Module",
	"downloadURL": "https://github.com/benkuper/EOS-OSC-Chataigne-Module/archive/master.zip",

	"scripts": [
		"EOSOSC.js"
	],


	"hasInput": true,
	"hasOutput": true,

	"hideDefaultCommands": false,

	"hideDefaultParameters": [
		"autoAdd",
		"splitArguments",
		"autoRange",
		"autoFeedback",
		"colorSendMode",
		"booleanSendMode",
		"clearValues",
		"useHierarchy"
	],

	"defaults": {
		"autoAdd": false,
		"OSC Input": {
			"localPort": 8001
		},
		"OSC Output": {
			"remotePort": 8000
		}
	},


	"parameters": {
		"Start Channel": {
			"type": "Integer"
		},

		"User": {
			"type": "Enum",
			"options": {
				"Console": "console",
				"Background": "background",
				"User ID": "id"
			},
			"default": "console"
		},
		"User ID": {
			"type": "Integer",
			"min": 1,
			"dependency": {
				"source": "User",
				"value": "id",
				"check": "equals",
				"action": "show"
			}
		}
	},

	"values": {
		"ActiveCueNo":{
			"type": "Float",
			"readOnly": true,
			"description": "Active cue number"
		},

		"ActiveCuelistNo":{
			"type": "Integer",
			"readOnly": true,
			"description": "Active cuelist number"
		},

		"PendingCueNo":{
			"type": "Float",
			"readOnly": true,
			"description": "Pending cue number"
		},

		"PendingCuelistNo":{
			"type": "Integer",
			"readOnly": true,
			"description": "Pending cuelist number"
		},

		"ActiveCueName":{
			"type": "String",
			"readOnly": true,
			"description": "Active cue text"
		},

		"ActiveCueLabel":{
			"type": "String",
			"readOnly": true,
			"description": "Active cue label"
		},

		"ActiveCueTime":{
			"type": "Float",
			"readOnly": true,
			"ui": "time",
			"description": "Active cue time"
		},

		"ActiveCuePercent":{
			"type": "Integer",
			"readOnly": true,
			"description": "Active cue percent complete"
		},

		"PendingCueName":{
			"type": "String",
			"readOnly": true,
			"description": "Pending cue text"
		},

		"PendingCueLabel":{
			"type": "String",
			"readOnly": true,
			"description": "Pending cue label"
		},

		"PendingCueTime":{
			"type": "Float",
			"readOnly": true,
			"ui": "time",
			"description": "Pending cue time"
		}
	},

	"commands": {
		"Set Value": {
			"menu": "",
			"callback": "valueCallback",
			"parameters": {
				"Target": {
					"type": "Enum",
					"options": {
						"All": "all",
						"One": "one",
						"Range": "range"
					}
				},

				"ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "one",
						"check": "equals",
						"action": "show"
					}
				},

				"Start ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				},

				"End ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				},

				"Value": {
					"type": "Float",
					"min": 0,
					"max": 1,
					"default": 0
				}
			}
		},

		"Set Color": {
			"menu": "",
			"callback": "colorCallback",
			"parameters": {
				"Target": {
					"type": "Enum",
					"options": {
						"All": "all",
						"One": "one",
						"Range": "range"
					}
				},

				"ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "one",
						"check": "equals",
						"action": "show"
					}
				},

				"Start ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				},

				"End ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				},

				"Color": {
					"type": "Color",
					"default": [1,0,0,1]
				}
			}
		},

		"Black Out": {
			"menu": "",
			"callback": "blackOutCallback",
			"parameters": {
				"Target": {
					"type": "Enum",
					"options": {
						"All": "all",
						"One": "one",
						"Range": "range"
					}
				},

				"ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "one",
						"check": "equals",
						"action": "show"
					}
				},

				"Start ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				},

				"End ID": {
					"type": "Integer",
					"default": 1,
					"dependency": {
						"source": "target",
						"value": "range",
						"check": "equals",
						"action": "show"
					}
				}
			}
		},

		"Gradient": {
			"menu": "Advanced",
			"callback": "gradientCallback",
			"parameters": {
				"Start ID": {
					"type": "Integer",
					"default": 1,
					"min": 0
				},

				"End ID": {
					"type": "Integer",
					"default": 1,
					"min": 0
				},

				"Start Color": {
					"type": "Color",
					"default": [1,0,0,1]
				},

				"End Color": {
					"type": "Color",
					"default": [0,0,1,1]
				}
			}
		},

		"Point": {
			"menu": "Advanced",
			"callback": "pointCallback",
			"parameters": {
				"Start ID": {
					"type": "Integer",
					"default": 1,
					"min": 0
				},

				"End ID": {
					"type": "Integer",
					"default": 1,
					"min": 0
				},

				"Position": {
					"type": "Float",
					"default": 0.5,
					"min": 0,
					"max": 1
				},

				"Size": {
					"type": "Float",
					"default": 0.1,
					"min": 0,
					"max": 1
				},

				"Fade": {
					"type": "Float",
					"default": 0.5,
					"min": 0,
					"max": 1
				},

				"Value": {
					"type": "Float",
					"min": 0,
					"max": 1,
					"default": 0
				}
			}
		}
	}
}
